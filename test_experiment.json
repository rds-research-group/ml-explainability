{
  "datasets": ["education"],
  "X": {
    "education": "matrices/portuguese_students/portuguese_students_X.csv",
    "housing": "matrices/housing_prices/X.csv"
  },
  "y": {
    "education": "matrices/portuguese_students/portuguese_students_y.csv",
    "housing": "matrices/housing_prices/y.csv"
  },
  "sub_groups": {
    "education": ["sex_female"]
  },
  "metrics": ["roc_auc_score", "precision_score@10"],
  "split_methods": ["StratifiedKFold@5", "train_test_split@25"],
  "split_random_states": [42],
  "hyperparameters": {
    "LR": {
      "penalty": ["l1", "l2"],
      "C": [0.1, 1, 10]
    },
    "DT": {
      "criterion": ["gini", "entropy"],
      "max_depth": [1, 2, 5],
      "max_features": ["sqrt", "log2"]
    }
  },
  "save_to_db": true,
  "postgres_nodelist": "cs118"
}
